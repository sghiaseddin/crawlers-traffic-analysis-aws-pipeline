    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowLambdaAndGlueAssumeRole",
        "Effect": "Allow",
        "Principal": {
          "Service": [
            "lambda.amazonaws.com",
            "glue.amazonaws.com"
          ]
